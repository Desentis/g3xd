# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/g3xd/[Ll]ibrary/
/g3xd/[Tt]emp/
/g3xd/[Oo]bj/
/g3xd/[Bb]uild/
/g3xd/[Bb]uilds/
/g3xd/[Ll]ogs/
/g3xd/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/g3xd/[Mm]emoryCaptures/

# Recordings can get excessive in size
/g3xd/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /g3xd/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/g3xd/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/g3xd/.vs/

# Gradle cache directory
/g3xd/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
ExportedObj/
/g3xd/.consulo/
/g3xd/*.csproj
/g3xd/*.unityproj
/g3xd/*.sln
/g3xd/*.suo
/g3xd/*.tmp
/g3xd/*.user
/g3xd/*.userprefs
/g3xd/*.pidb
/g3xd/*.booproj
/g3xd/*.svd
/g3xd/*.pdb
/g3xd/*.mdb
/g3xd/*.opendb
/g3xd/*.VC.db

# Unity3D generated meta files
/g3xd/*.pidb.meta
/g3xd/*.pdb.meta
/g3xd/*.mdb.meta

# Unity3D generated file on crash reports
/g3xd/sysinfo.txt

# Builds
/g3xd/*.apk
/g3xd/*.aab
/g3xd/*.unitypackage
/g3xd/*.app

# Crashlytics generated file
/g3xd/crashlytics-build.properties

# Packed Addressables
/g3xd/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/g3xd/[Aa]ssets/[Ss]treamingAssets/aa.meta
/g3xd/[Aa]ssets/[Ss]treamingAssets/aa/*
